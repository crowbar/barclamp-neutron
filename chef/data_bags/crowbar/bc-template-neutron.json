{
  "id": "bc-template-neutron",
  "description": "API-enabled, pluggable virtual network service for OpenStack",
  "attributes": {
    "neutron": {
      "service_user": "neutron",
      "gitrepo": "http://github.com/openstack/neutron.git",
      "git_instance": "",
      "git_refspec": "stable/icehouse",
      "use_gitbarclamp": true,
      "use_pip_cache": true,
      "use_gitrepo": false,
      "use_virtualenv": true,
      "rabbitmq_instance": "none",
      "keystone_instance": "none",
      "pfs_deps": [
        "libmysqlclient-dev",
        "pip://setuptools-git",
        "pip://distribute==0.6.28",
        "pip://MySQL-python",
        "pip://pyudev",
        "pip://six"
      ],
      "debug": false,
      "verbose": true,
      "dhcp_domain": "openstack.local",
      "use_ml2": true,
      "use_lbaas": true,
      "use_infoblox": false,
      "networking_mode": "gre",
      "networking_plugin": "openvswitch",
      "num_vlans": 2000,
      "use_syslog": false,
      "database_instance": "none",
      "db": {
        "database": "neutron",
        "user": "neutron"
      },
      "sql": {
        "min_pool_size": 30,
        "max_pool_overflow": 10,
        "pool_timeout": 30
      },
      "vmware": {
        "user": "",
        "password": "",
        "controllers": "",
        "tz_uuid": "",
        "l3_gw_uuid": ""
      },
      "ssl": {
        "certfile": "/etc/neutron/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/neutron/ssl/private/signing_key.pem",
        "generate_certs": false,
        "insecure": false,
        "cert_required": false,
        "ca_certs": "/etc/neutron/ssl/certs/ca.pem"
      },
      "api": {
        "protocol": "http",
        "service_port": 9696,
        "service_host": "0.0.0.0"
      },
      "infoblox": {
        "ib_wapi": "",
        "ib_username": "admin",
        "ib_password": "",
        "ib_members": "",
        "ib_dnsserver": [],
        "use_host_records_for_ip_allocation": true,
        "bind_dns_records_to_fixed_address": "record:a,record:ptr",
        "unbind_dns_records_from_fixed_address": "record:a,record:ptr",
        "delete_dns_records_associated_with_fixed_address": "record:a,record:ptr,record:txt",
        "default_condition": "tenant",
        "default_network_view": "default",
        "default_dns_view": "default",
        "default_dhcp_members": "\"<next-available-member>\"",
        "default_dns_members": "\"<next-available-member\"",
        "default_domain_suffix_pattern": "{subnet_name}.cloud-test.com",
        "default_hostname_pattern": "host-{ip_address}",
        "external_condition": "global",
        "external_network_view": "default",
        "external_dns_view": "default",
        "external_dhcp_members": "\"<next-available-member>\"",
        "external_dns_members": "\"<next-available-member>\"",
        "external_domain_suffix_pattern": "{subnet_name}.cloud-test.com",
        "external_hostname_pattern": "host-{ip_address}"
      }
    }
  },
  "deployment": {
    "neutron": {
      "crowbar-revision": 0,
      "crowbar-applied": false,
      "schema-revision": 10,
      "element_states": {
        "neutron-server": [ "readying", "ready", "applying" ],
        "neutron-l3": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
         ["neutron-server" ],
         ["neutron-l3" ]
      ],
      "element_run_list_order": {
        "neutron-server": 94,
        "neutron-l3": 95
      },
      "config": {
        "environment": "neutron-config-base",
        "mode": "full",
        "transitions": false,
        "transition_list": []
      }
    }
  }
}
