{
  "id": "bc-template-neutron",
  "description": "API-enabled, pluggable virtual network service for OpenStack",
  "attributes": {
    "neutron": {
      "service_user": "neutron",
      "gitrepo": "http://github.com/openstack/neutron.git",
      "git_instance": "",
      "git_refspec": "stable/icehouse",
      "use_gitbarclamp": true,
      "use_pip_cache": true,
      "use_gitrepo": false,
      "use_virtualenv": true,
      "rabbitmq_instance": "none",
      "keystone_instance": "none",
      "pfs_deps": [
        "libmysqlclient-dev",
        "pip://setuptools-git",
        "pip://distribute==0.6.28",
        "pip://MySQL-python",
        "pip://pyudev",
        "pip://six"
      ],
      "debug": false,
      "verbose": true,
      "dhcp_domain": "openstack.local",
      "use_ml2": true,
      "use_lbaas": true,
      "networking_mode": "gre",
      "networking_plugin": "openvswitch",
      "num_vlans": 2000,
      "networks": {
        "fixed": {
          "conduit": "intf1",
          "vlan": 500,
          "use_vlan": true,
          "external": false,
          "subnet": "10.168.1.0",
          "broadcast": "10.168.1.255",
          "netmask": "255.255.255.0",
          "ranges": {
            "dhcp": {
              "end": "10.168.1.254",
              "start": "10.168.1.1"
            }
          },
          "router": "10.168.1.1"
        },
        "floating": {
          "conduit": "intf1",
          "vlan": 300,
          "use_vlan": false,
          "external": true,
          "subnet": "192.168.126.128",
          "broadcast": "192.168.126.255",
          "netmask": "255.255.255.128",
          "ranges": {
            "host": {
              "end": "192.168.126.254",
              "start": "192.168.126.129"
            }
          },
          "router": "192.168.126.1"
        }
      },
      "use_syslog": false,
      "database_instance": "none",
      "db": {
        "database": "neutron",
        "user": "neutron"
      },
      "sql": {
        "min_pool_size": 30,
        "max_pool_overflow": 10,
        "pool_timeout": 30
      },
      "vmware": {
        "user": "",
        "password": "",
        "port": "443",
        "controllers": "",
        "tz_uuid": "",
        "l3_gw_uuid": ""
      },
      "ssl": {
        "certfile": "/etc/neutron/ssl/certs/signing_cert.pem",
        "keyfile": "/etc/neutron/ssl/private/signing_key.pem",
        "generate_certs": false,
        "insecure": false,
        "cert_required": false,
        "ca_certs": "/etc/neutron/ssl/certs/ca.pem"
      },
      "api": {
        "protocol": "http",
        "service_port": 9696,
        "service_host": "0.0.0.0"
      }
    }
  },
  "deployment": {
    "neutron": {
      "crowbar-revision": 0,
      "crowbar-applied": false,
      "schema-revision": 11,
      "element_states": {
        "neutron-server": [ "readying", "ready", "applying" ],
        "neutron-l3": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
         ["neutron-server" ],
         ["neutron-l3" ]
      ],
      "element_run_list_order": {
        "neutron-server": 94,
        "neutron-l3": 95
      },
      "config": {
        "environment": "neutron-config-base",
        "mode": "full",
        "transitions": false,
        "transition_list": []
      }
    }
  }
}
